{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "Small calculator website to help figure out how much weight you should be lifting to hit your max",
  "version": "0.1.0",
  "author": "Zack Smith <zack.t.smith37@gmail.com",
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "gatsby": "^2.21.22",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-sharp": "^2.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "2.0.5",
    "xo": "^0.30.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --open",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "xo --quiet",
    "lint:fix": "yarn lint --fix",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "pre-commit": "lint-staged",
    "pre-push": "yarn lint"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "xo": {
    "prettier": true,
    "env": [
      "browser",
      "webextensions"
    ],
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "capitalized-comments": "off"
    },
    "ignores": [
      "background-v1-1-backup.js",
      "background-v1-backup.js"
    ]
  }
}
