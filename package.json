{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "Small calculator website to help figure out how much weight you should be lifting to hit your max",
  "version": "0.1.0",
  "author": "Zack Smith <zack.t.smith37@gmail.com",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --open",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "xo --quiet",
    "lint:fix": "yarn lint --fix",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -u \"Zack Smith <zack.t.smith37@gmail.com>\""
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "eslint-plugin-prettier": "^3.1.3",
    "gatsby": "^2.21.22",
    "gatsby-image": "^2.4.3",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.2",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-sharp": "^2.5.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-react": "^7.20.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "2.0.5",
    "xo": "^0.30.0"
  },
  "keywords": [
    "maxout",
    "block",
    "calculator"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zack37/maxout-block-calculator"
  },
  "bugs": {
    "url": "https://github.com/zack37/maxout-block-calculator/issues"
  },
  "husky": {
    "pre-commit": "lint-staged",
    "pre-push": "yarn lint"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "xo": {
    "prettier": true,
    "extends": "xo-react",
    "env": [
      "browser"
    ],
    "plugins": [
      "unicorn",
      "react"
    ],
    "rules": {
      "capitalized-comments": "off",
      "react/jsx-pascal-case": "off",
      "unicorn/filename-case": "off"
    }
  }
}
