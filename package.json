{
  "name": "maxout-block-calculator",
  "private": true,
  "description": "Small calculator website to help figure out how much weight you should be lifting to hit your max",
  "engines": {
    "node": "14"
  },
  "version": "0.1.0",
  "author": "Zack Smith <zack.t.smith37@gmail.com",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop --open",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "xo --quiet",
    "lint:fix": "yarn lint --fix",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "deploy": "yarn build && gh-pages -d public -u \"Zack Smith <zack.t.smith37@gmail.com>\"",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "gatsby": "^3.0.0",
    "gatsby-image": "^3.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-sharp": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.0.0"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.24.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^3.1.0",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "xo": "^0.38.2"
  },
  "keywords": [
    "maxout",
    "block",
    "calculator"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zack37/maxout-block-calculator"
  },
  "bugs": {
    "url": "https://github.com/zack37/maxout-block-calculator/issues"
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix"
    ],
    "*.{js,json,md}": [
      "prettier --write"
    ]
  },
  "xo": {
    "prettier": true,
    "extends": "xo-react",
    "envs": [
      "browser"
    ],
    "plugins": [
      "unicorn",
      "react"
    ],
    "rules": {
      "capitalized-comments": "off",
      "import/extensions": "off",
      "react/jsx-pascal-case": "off",
      "unicorn/filename-case": "off"
    }
  }
}
